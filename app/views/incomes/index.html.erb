<div class="well">
<%= link_to "New Income", incomes_new_path, class: "btn btn-primary" %>
<%= link_to "Back", root_path, class: "btn btn-primary" %>
</div>
<table class="table table-bordered table-striped">
    <% if @incomes.count == 0 %>
      <tr>
        <td colspan="3">
          No income found.  Click the button above to add a new one.
        </td>
      </tr>
    <% else %>
  <thead>
    <tr>
      <th>Type Of Income</th>
      <th>Amount</th>
      <th>Date</th>
      <th>&nbsp;</th>
    </tr>
  </thead>
  <tbody>
      <% @incomes.each do |income| %>
        <tr>
          <td><%= income.type_of_income_and_expense %></td>
          <td><%= income.amount %></td>
          <td><%= income.enter_date %></td>
          <td><%= link_to "Show", incomes_show_path(income), class: "btn btn-default" %></td>
          <td><%= link_to "Edit", incomes_edit_path(income), class: "btn btn-default" %></td>
          <td><%= link_to 'Delete', transaction_path(income), :method=>:delete, class: "btn btn-danger", :data => {:confirm => 'Are you sure?'} %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>